{
  "name": "server",
  "version": "1.0.0",
  "description": "Fastify server with MySQL2 and TypeScript using ES modules",
  "type": "module",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit",
    "pm2:dev": "pm2 start ecosystem.config.cjs --only project-x-server-dev",
    "pm2:prod": "npm run build && pm2 start ecosystem.config.cjs --only project-x-server",
    "pm2:stop": "pm2 stop project-x-server project-x-server-dev",
    "pm2:restart": "pm2 restart project-x-server",
    "pm2:reload": "pm2 reload project-x-server",
    "pm2:logs": "pm2 logs project-x-server",
    "pm2:status": "pm2 status",
    "pm2:delete": "pm2 delete project-x-server project-x-server-dev"
  },
  "keywords": [
    "fastify",
    "mysql2",
    "typescript",
    "esmodules"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fastify/compress": "^8.0.1",
    "@fastify/cookie": "^11.0.2",
    "@fastify/cors": "^11.1.0",
    "@fastify/helmet": "^13.0.2",
    "@fastify/multipart": "^9.3.0",
    "@fastify/rate-limit": "^10.3.0",
    "@fastify/sensible": "^6.0.3",
    "@fastify/static": "^8.3.0",
    "@fastify/swagger": "^9.4.2",
    "@fastify/swagger-ui": "^5.2.1",
    "axios": "^1.13.2",
    "bcryptjs": "^3.0.3",
    "dotenv": "^17.2.3",
    "fastify": "^5.6.2",
    "fastify-type-provider-zod": "^6.1.0",
    "ioredis": "^5.4.1",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.15.3",
    "node-cron": "^4.2.1",
    "sharp": "^0.34.5",
    "uuid": "^11.1.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.10.1",
    "@types/uuid": "^10.0.0",
    "fastify-plugin": "^5.1.0",
    "pino-pretty": "^13.1.2",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  }
}
