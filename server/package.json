{
  "name": "server",
  "version": "1.0.0",
  "description": "Fastify server with MySQL2 and TypeScript using ES modules",
  "type": "module",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "pm2": "pm2 start ecosystem.config.cjs --only project-x-server-dev",
    "stop-pm2": "pm2 stop project-x-server-dev",
    "start-pm2": "pm2 start ecosystem.config.cjs --only project-x-server",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "fastify",
    "mysql2",
    "typescript",
    "esmodules"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fastify/cookie": "^11.0.2",
    "@fastify/cors": "^11.1.0",
    "@fastify/helmet": "^13.0.2",
    "@fastify/rate-limit": "^10.3.0",
    "@fastify/sensible": "^6.0.3",
    "axios": "^1.13.2",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.3",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express-rate-limit": "^8.2.1",
    "express-session": "^1.18.2",
    "express-validator": "^7.3.0",
    "fastify": "^5.6.2",
    "fastify-type-provider-zod": "^6.1.0",
    "ioredis": "^5.4.1",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "mysql2": "^3.15.3",
    "node-cron": "^4.2.1",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@fastify/type-provider-json-schema-to-ts": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.10.1",
    "concurrently": "^9.2.1",
    "fastify-plugin": "^5.1.0",
    "nodemon": "^3.1.11",
    "pino-pretty": "^13.1.2",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  }
